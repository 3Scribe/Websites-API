      "gulp-concat": "~2.6",
      "gulp-ejs": "~3.1",
      "gulp-htmlhint": "~1.0",
      "gulp-if": "~2.0",
      "gulp-jshint": "~2.1",
      "gulp-live-server": "~0.0",
      "gulp-open": "~2.1",
      "gulp-prettify": "~0.5",
      "gulp-rename": "~1.2",
      "gulp-sass": "~3.1",
      "gulp-uglify": "~3.0",
      "gulp-util": "~3.0",
      "gulp-w3cjs": "~1.3",
      "highlight.js": "~9.12",
      "js-yaml": "~3.10",
      "jshint": "~2.9",
      "marked": "~0.3",
      "merge-stream": "~1.0",
      "node-sass": "~4.7"
   }
